{
	"compilerOptions": {
		"sourceMap": true,
		"outDir": "assets",
		"noImplicitAny": false,
		"module": "ES6",
		"target": "ES6",
		"jsx": "preserve",
		"allowJs": true,
		"moduleResolution": "Node",
		"esModuleInterop": true,
	},
	"include": [
		"src/**/*.ts",
		"src/**/*.d.ts",
		"src/**/*.vue",
		"src/main.js"
	],
	"exclude": [
		"node_modules"
	]
}